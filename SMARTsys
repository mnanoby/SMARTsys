    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        try {
            // TODO add your handling code here:
            String nim = null, ambil;
            Statement stm;
            String paketBimbel = null;
            String jeniskelamin = null;

//            String ambilBiaya = jTextField6.getText();
//            double biaya = Double.parseDouble(ambilBiaya);
//            DecimalFormat df = (DecimalFormat) DecimalFormat.getCurrencyInstance();
//            DecimalFormatSymbols dfs = new DecimalFormatSymbols();
//            dfs.setCurrencySymbol("");
//            dfs.setMonetaryDecimalSeparator(',');
//            dfs.setGroupingSeparator('.');
//            df.setDecimalFormatSymbols(dfs);
//            String hasil = df.format(biaya);
//            String hsl = jTextField6.setText(hasil);

            //Button Group 1
            if (jRadioButton1.isSelected()) {
                jeniskelamin = jRadioButton1.getText();
            } else {
                jeniskelamin = jRadioButton2.getText();

            }

            // Button Group 2
            if (jRadioButton3.isSelected()) {
                paketBimbel = jRadioButton3.getText();
            } else {
                paketBimbel = jRadioButton4.getText();

            }

            stm = (Statement) cn.createStatement();
            rs = stm.executeQuery("SELECT NomorInduk FROM datasiswa WHERE NomorInduk = '" + (jComboBox1.getSelectedItem() + jTextField2.getText()) + "'");
            while (rs.next()) {
                nim = rs.getString("NomorInduk");
            }
            ambil = (jComboBox1.getSelectedItem() + jTextField2.getText());
            if (ambil.equals(nim)) {
                int reply = JOptionPane.showConfirmDialog(null, "Apakah Anda Yakin Ingin Mengubah Data ini ?", "NOMOR INDUK TELAH DIGUNAKAN!", JOptionPane.YES_NO_OPTION);
                if (reply == JOptionPane.YES_OPTION) {
                    stm.executeUpdate("update datasiswa set Nama = '" + jTextField1.getText() + "',"
                            + "JenisKelamin ='" + jeniskelamin + "',"
                            + "Alamat = '" + jTextArea1.getText() + "',"
                            + "SekolahAsal = '" + jTextField5.getText() + "',"
                            + "CPSiswa = '" + jTextField3.getText() + "',"
                            + "CPOrangTua = '" + jTextField4.getText() + "',"
                            + "NomorInduk = '" + jComboBox1.getSelectedItem() + jTextField2.getText() + "',"
                            + "Jurusan = '" + jComboBox3.getSelectedItem() + "',"
                            + "Kelas = '" + jLabel13.getText() + "',"
                            + "Tagihan = '" + jTextField6.getText() + "',"
                            + "PaketBimbel = '" + paketBimbel + "'"
                            + "Where NomorInduk = '" + ambil + "'");
                    JOptionPane.showMessageDialog(this, "Data Berhasil DiUbah!");
                    kosongDS();
                    fHU.konekDataSiswa();
                }
            } else {

                if ((jTextField1.getText() == null)
                        || (jTextField2.getText() == null)
                        || (jTextField3.getText() == null)
                        || (jTextField4.getText() == null)
                        || (jTextField5.getText() == null)
                        || jTextArea1.getText() == null
                        || (jComboBox1.getSelectedIndex() == 0)
                        || (jComboBox3.getSelectedIndex() == 0)
                        || (jLabel13.getText() == null)
                        || (jeniskelamin == null)) {
                    JOptionPane.showMessageDialog(rootPane, "Mohon Semua Kolom Diisi dengan Lengkap");
                } else {
                    stm.executeUpdate("INSERT into datasiswa(Nama, JenisKelamin, Alamat, SekolahAsal, CPSiswa, CPOrangTua, NomorInduk,  Kelas,Jurusan,Tagihan, PaketBimbel) "
                            + "values ("
                            + "'" + jTextField1.getText() + "',"
                            + "'" + jeniskelamin + "',"
                            + "'" + jTextArea1.getText() + "',"
                            + "'" + jTextField5.getText() + "',"
                            + "'" + jTextField3.getText() + "',"
                            + "'" + jTextField4.getText() + "',"
                            + "('" + jComboBox1.getSelectedItem() + jTextField2.getText() + "'),"
                            + "'" + jLabel13.getText() + "',"
                            + "'" + jComboBox3.getSelectedItem() + "',"
                            + "'" + jTextField6.getText() + "',"
                            + "'" + paketBimbel + "')");
                    JOptionPane.showMessageDialog(rootPane, "Data berhasil ditambahkan ");
                    kosongDS();
                    fHU.konekDataSiswa();
                }
            }
        } catch (SQLException ex) {
            Logger.getLogger(CRUD.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                        

