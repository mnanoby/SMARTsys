    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        String kodepel = null;
        try {
            Statement stm;
            stm = (Statement) cn.createStatement();
            rs = stm.executeQuery("SELECT KodePelajaran from matapelajaran WHERE NamaMataPelajaran= '" + jComboBox6.getSelectedItem() + "' ");
            rs.next();
            kodepel = rs.getString("KodePelajaran");
            stm = (Statement) cn.createStatement();
            stm.executeUpdate("insert into tryout(NomorIndukDS, KodeMapelMP, TglPel, Nilai) "
                    + "values ("
                    + "('" + jComboBox5.getSelectedItem() + jTextField8.getText() + "'),"
                    + "'" + kodepel + "',"
                    + "'" + tanggal2 + "',"
                    + "'" + jTextField9.getText() + "')");
            JOptionPane.showMessageDialog(rootPane, "Data berhasil ditambahkan ");
            kosongN();
        } catch (Exception e) {
            JOptionPane.showMessageDialog(rootPane, "Error " + e);
        }
    }                                        

