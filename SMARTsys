    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String idJP;
        try {
            Statement stm;
            
            //jenispembayaran
            stm = (Statement) cn.createStatement();
            rs = stm.executeQuery("select Id from jenispembayaran jp Where NamaJP = '" + jComboBox8.getSelectedItem() + "' ");
            rs.next();
            idJP = rs.getString("Id").toString();
            
            //insert into datapembayaran
                    stm = (Statement) cn.createStatement();
                    stm.executeUpdate("insert into datapembayaran(NoKwitansi, TglBayar, Nominal, NomorIndukDS, IdJP,KodeDivisiAdm) "
                            + "values ("
                            + "'" + jTextField10.getText() + "',"
                            + "'" + tanggal3 + "',"
                            + "'" + jTextField12.getText() + "',"
                            + "('" + jComboBox7.getSelectedItem() + jTextField11.getText() + "'),"
                            + "'" + idJP + "',"
                            + "'" + LogIn.kode + "')");
                    JOptionPane.showMessageDialog(rootPane, "Data berhasil ditambahkan ");
                    kosongPemb();
       } catch (SQLException e) {
            JOptionPane.showMessageDialog(rootPane, "Error " + e);
        }        // TODO add your ha
    }                                        

