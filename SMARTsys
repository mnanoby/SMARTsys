    private void jDateChooser1PropertyChange(java.beans.PropertyChangeEvent evt) {                                             
        // TODO add your handling code here:
        if (jDateChooser1.getDate() != null) {
            SimpleDateFormat simpleDateFormat = new SimpleDateFormat("dd-MM-yyyy");
            tanggal1 = simpleDateFormat.format(jDateChooser1.getDate());
        }
    }                                            

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        String Id;
        String kehadiran;
        try {
            if (jRadioButton6.isSelected()) {
                kehadiran = jRadioButton6.getText();
            } else {
                kehadiran = jRadioButton5.getText();
            }

            Statement stm;
            stm = (Statement) cn.createStatement();
            rs = stm.executeQuery("SELECT KodePelajaran FROM matapelajaran Where NamaMataPelajaran = '" + jComboBox4.getSelectedItem() + "'");
            rs.next();
            Id = rs.getString("KodePelajaran");
            stm.executeUpdate("INSERT INTO presensi(NomorIndukDS, KodeMapelMP, TglPre, Kehadiran)"
                    + "VALUES ("
                    + "('" + jComboBox2.getSelectedItem() + jTextField7.getText() + "'),"
                    + "'" + Id + "',"
                    + "'" + tanggal1 + "',"
                    + "'" + kehadiran + "')");
            JOptionPane.showMessageDialog(rootPane, "Data berhasil ditambahkan ");
        } catch (Exception e) {
            JOptionPane.showMessageDialog(rootPane, "Error " + e);
        }

    }                                        

