    Connection cn = null;
    ResultSet rs = null;
    DefaultTableModel tabModel;
    String tanggal1, tanggal2, tanggal3;

    void koneksi() {
        try {
            Class.forName("com.mysql.jdbc.Driver");
            cn = (Connection) DriverManager.getConnection("jdbc:mysql://localhost:3306/kp?zeroDateTimeBehavior=convertToNull", "root", "");
        } catch (ClassNotFoundException e) {
            e.printStackTrace();
        } catch (SQLException e) {
            e.printStackTrace();
        }
    }
// CRUD data siswa

    void konekDataSiswa() {
        try {
            Class.forName("com.mysql.jdbc.Driver");
            cn = (Connection) DriverManager.getConnection("jdbc:mysql://localhost:3306/kp?zeroDateTimeBehavior=convertToNull", "root", "");
            Object[] judul = {"Nomor Induk", "Nama", "Jenis Kelamin", "Alamat", "Sekolah Asal", "CP Siswa", "CP Orang Tua", "Jurusan", "Kelas", "Tagihan", "Paket Bimbel"};
            tabModel = new DefaultTableModel(null, judul);
            tampilDS();
        } catch (ClassNotFoundException e) {
            e.printStackTrace();
        } catch (SQLException e) {
            e.printStackTrace();
        }
    }

    void ButtonGroup1() {
        String jk;
        jk = "Laki - laki";
        if (tabModel.getValueAt(fHU.jTable1.getSelectedRow(), 3).equals(jk)) {
            jRadioButton1.setSelected(rootPaneCheckingEnabled);
        } else {
            jRadioButton2.setSelected(rootPaneCheckingEnabled);

        }
    }

    void ButtonGroup2() {
        String pb;
        pb = "Reguler";
        if (tabModel.getValueAt(fHU.jTable1.getSelectedRow(), 10).equals(pb)) {
            jRadioButton3.setSelected(rootPaneCheckingEnabled);
        } else {
            jRadioButton4.setSelected(rootPaneCheckingEnabled);

        }
    }

    void kode() {
        Object nim;
        nim = tabModel.getValueAt(fHU.jTable1.getSelectedRow(), 0);
        String str = nim.toString();
        char[] chr = str.toCharArray();


        if (chr.length <= 15) {
            String s1 = new String(chr, 12, 3);
            String s2 = new String(chr, 0, 12);
            jComboBox1.setSelectedItem(s2);
            jTextField2.setText(s1);
        } else if (chr.length > 15) {
            String s3 = new String(chr, 0, 13);
            String s4 = new String(chr, 13, 3);
            jComboBox1.setSelectedItem(s3);
            jTextField2.setText(s4);
        }
    }

    void ambilData() {
        HalamanUtama fHU = new HalamanUtama();
        fHU.fCRUD = this;
        jTextField1.setText(Nama);
        jTextField5.setText(SekolahAsal);
        jTextField3.setText(CPSiswa);
        jTextField4.setText(CPOrtu);
        jTextField6.setText(Tagihan);
        jTextArea1.setText(Alamat);
        jComboBox3.setSelectedItem(Jurusan);
        jLabel13.setText(Kelas);
    }

    public void kosongDS() {
        buttonGroup1.clearSelection();
        jRadioButton3.setSelected(rootPaneCheckingEnabled);
        jTextField1.setText("");
        jTextArea1.setText("");
        jComboBox1.setSelectedIndex(0);
        jComboBox3.setSelectedIndex(0);
        jLabel13.setText("");
        jTextField2.setText("");
        jTextField3.setText("");
        jTextField4.setText("");
        jTextField5.setText("");
        jTextField6.setText("");
    }

    public void NomorInduk() {
        if ((jComboBox1.getSelectedIndex() == 1)) {
            jLabel13.setText("3 SD");
            jComboBox3.setSelectedIndex(3);
            jComboBox3.setEnabled(false);
        } else if (jComboBox1.getSelectedIndex() == 2) {
            jLabel13.setText("4 SD");
            jComboBox3.setSelectedIndex(3);
            jComboBox3.setEnabled(false);
        } else if (jComboBox1.getSelectedIndex() == 3) {
            jLabel13.setText("5 SD");
            jComboBox3.setSelectedIndex(3);
            jComboBox3.setEnabled(false);
        } else if (jComboBox1.getSelectedIndex() == 4) {
            jLabel13.setText("6 SD");
            jComboBox3.setSelectedIndex(3);
            jComboBox3.setEnabled(false);
        } else if (jComboBox1.getSelectedIndex() == 5) {
            jLabel13.setText("7 SMP");
            jComboBox3.setSelectedIndex(3);
            jComboBox3.setEnabled(false);
        } else if (jComboBox1.getSelectedIndex() == 6) {
            jLabel13.setText("8 SMP");
            jComboBox3.setSelectedIndex(3);
            jComboBox3.setEnabled(false);
        } else if (jComboBox1.getSelectedIndex() == 7) {
            jLabel13.setText("9 SMP");
            jComboBox3.setSelectedIndex(3);
            jComboBox3.setEnabled(false);
        } else if (jComboBox1.getSelectedIndex() == 8) {
            jLabel13.setText("10 SMA");
            jComboBox3.setEnabled(rootPaneCheckingEnabled);
        } else if (jComboBox1.getSelectedIndex() == 9) {
            jLabel13.setText("11 SMA");
            jComboBox3.setEnabled(rootPaneCheckingEnabled);
        } else if (jComboBox1.getSelectedIndex() == 10) {
            jLabel13.setText("12 SMA");
            jComboBox3.setEnabled(rootPaneCheckingEnabled);
        } else if (jComboBox1.getSelectedIndex() == 11) {
            jLabel13.setText("12 SMA");
            jComboBox3.setEnabled(rootPaneCheckingEnabled);
        } else {
            jLabel13.setText("");
            jComboBox3.setEnabled(rootPaneCheckingEnabled);
        }
    }

    // CRUD Pembayaran
    void kosongPemb() {
        jTextField10.setText("");
        jTextField11.setText("");
        jTextField12.setText("");
        jComboBox7.setSelectedIndex(0);
        jComboBox8.setSelectedIndex(0);
    }
    
     void jcom8() {
        Statement stm;
        try {
            stm = (Statement) cn.createStatement();
            rs = stm.executeQuery("SELECT * FROM jenispembayaran");
            while (rs.next()) {
                jComboBox8.addItem(rs.getString("NamaJP"));
            }
        } catch (SQLException ex) {
        }
    }
    //CRUD Presensi 

    void jcom2() {
        Statement stm;
        try {
            stm = (Statement) cn.createStatement();
            rs = stm.executeQuery("SELECT * FROM matapelajaran");
            while (rs.next()) {
                jComboBox4.addItem(rs.getString("NamaMataPelajaran"));
            }
        } catch (SQLException ex) {
        }
    }

    //CRUD Nilai
    void kosongN() {
        jTextField9.setText("");
        jComboBox6.setSelectedIndex(0);
    }

